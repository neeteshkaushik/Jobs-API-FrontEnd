<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>
  <body class="container">
    <header><%- include('../partials/header'); %></header>

    <main>
      <form>
        <label for="email">Email</label>
        <input type="email" name="email" id="email" placeholder="Enter email" />
